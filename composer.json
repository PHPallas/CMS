{
    "name": "phpallas/cms",
    "authors": [
        {
            "name": "Sina Kuhestani",
            "email": "sinakuhestani@gmail.com"
        }
    ],
    "description": "Free Open Source Content Management System written in PHP",
    "type": "project",
    "license": "MIT",
    "config": {
        "vendor-dir": "lib",
        "bin-dir": "build/bin",
        "data-dir": "build/data",
        "cache-vcs-dir": "build/cache/vcs",
        "cache-dir": "build/cache",
        "archive-dir": "build/archive",
        "cache-files-dir": "build/cache/files",
        "cache-repo-dir": "build/cache/repo"
    },
    "autoload": {
        "psr-4": {
            "Phpallas\\Cms\\": "src/",
            "Phpallas\\SampleComponent\\": "com/SampleComponent/src/"
        }
    },
    "repositories": {},
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^11"
    }
}
